class DoublyLinkedList {
    
    private ListNode head;
    private ListNode tail;
    private int length;
    
    private class ListNode {
        private ListNode next;
        private ListNode previous;
        private int data;
        
        public ListNode(int data) {
            this.data = data;
            this.next = null;
            this.previous = null;
        }
    }
    
    public DoublyLinkedList() {
        this.length = 0;
        this.head = null;
        this.tail = null;
    }
    
    public int length() {
        return length;
    }
    
    boolean isEmpty() {
        return head == null;
    }
}